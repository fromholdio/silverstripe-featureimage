{
  "name": "fromholdio/silverstripe-featureimage",
  "type": "silverstripe-vendormodule",
  "description": "Adds feature image to pages and a fallback to SiteConfig, with inheritance/fallbacks",
  "license": "BSD-3-Clause",
  "keywords": ["silverstripe", "cms", "feature-image", "image", "page"],
  "homepage": "https://github.com/fromholdio/silverstripe-featureimage",
  "support": {
    "issues": "https://github.com/fromholdio/silverstripe-featureimage/issues"
  },
  "authors": [{
    "name": "Luke Fromhold",
    "homepage": "https://fromhold.io"
  }],
  "require": {
    "php": ">=5.6",
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/cms": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Fromholdio\\FeatureImage\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}